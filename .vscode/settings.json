{
	"search.exclude": {
		"export/**/*.*": true
	},
	"[haxe]": {
		"editor.formatOnSave": true,
		"editor.formatOnSaveMode":"modifications",
		"editor.formatOnPaste": false,
		"editor.codeActionsOnSave": {
			"source.sortImports": "explicit"
		}
	},
	"haxe.enableExtendedIndentation": true,
	"files.exclude": {
		"**/*.rpyc": true,
		"**/*.rpa": true,
		"**/*.rpymc": true,
		"**/cache/": true
	}
}
